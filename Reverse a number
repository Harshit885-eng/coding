#include <iostream>
using namespace std;

int main() 
{
  int n;
  cin>>n;
  int ans=0;
  while(n>0)
  {
      int m=n%10;
      ans=ans*10+m;
      n=n/10;
  }
  cout<<ans<<endl;
}



#include <iostream>
using namespace std;

int main() 
{
  int n;
  cin>>n;
  while(n>0)
  {
      int m=n%10;
      cout<<m;
      n=n/10;
  }
}
